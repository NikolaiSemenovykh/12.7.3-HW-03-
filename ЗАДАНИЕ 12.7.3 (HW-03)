per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
money = float(input("Введите сумму вклада "))
deposit = list(per_cent.values())
deposit_1,deposit_2,deposit_3,deposit_4 = deposit[0]*money/100,deposit[1]*money/100,deposit[2]*money/100,deposit[3]*money/100
deposit = [deposit_1,deposit_2,deposit_3,deposit_4]
print("Накопленные средства соответственно",deposit)
print("Максимальная сумма, которую вы можете заработать —",max(deposit))
